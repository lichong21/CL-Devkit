{
  "name": "vite-for-extension",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "npm run clean && cross-env NODE_ENV=development && npm run build && run-p dev:*",
    "dev:prepare": "esno scripts/prepare.ts",
    "dev:background": "vite build --config vite.config.background.ts --mode development --watch",
    "dev:content": "npm run build:content -- --mode development",
    "dev:web": "vite --config vite.config.ts",
    "lint": "eslint . --ext js,ts,vue --report-unused-disable-directives",
    "build": "vue-tsc -b && vite build",
    "build:extension": "vue-tsc -b && vite build && npm run build:content && esno scripts/build-manifest.ts",
    "build:background": "vite build --config vite.config.background.ts",
    "build:content": "vite build --config vite.config.content.ts",
    "dev1:prepare": "npm run build && npm run build:background && NODE_ENV=development esno scripts/prepare.ts ",
    "clean": "rimraf dist/*"
  },
  "dependencies": {
    "@arco-design/web-vue": "^2.57.0",
    "chokidar": "^4.0.3",
    "chrome-types": "^0.1.353",
    "fs-extra": "^11.3.0",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@types/node": "^24.0.3",
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "@typescript-eslint/parser": "^8.34.0",
    "@unocss/preset-attributify": "^66.3.3",
    "@unocss/preset-icons": "^66.3.3",
    "@unocss/preset-uno": "^66.3.3",
    "@unocss/preset-web-fonts": "^66.3.3",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/tsconfig": "^0.7.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.28.0",
    "eslint-plugin-vue": "^10.2.0",
    "esno": "^4.8.0",
    "globals": "^16.2.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1",
    "sass-embedded": "^1.89.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.34.0",
    "unocss": "^66.3.3",
    "vite": "^6.3.5",
    "vue-tsc": "^2.2.8"
  }
}
