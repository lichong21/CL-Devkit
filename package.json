{
  "name": "vite-for-extension",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "lint": "eslint . --ext js,ts,vue --report-unused-disable-directives",
    "build": "vue-tsc -b && vite build",
    "build:extension": "vue-tsc -b && vite build && node scripts/build-manifest.js",
    "build:background": "vite build --config vite.config.background.ts",
    "dev:prepare": "npm run build &&  npm run build:background && NODE_ENV=development esno scripts/prepare.ts  "
  },
  "dependencies": {
    "chokidar": "^4.0.3",
    "chrome-types": "^0.1.353",
    "fs-extra": "^11.3.0",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@types/node": "^24.0.1",
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "@typescript-eslint/parser": "^8.34.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.28.0",
    "eslint-plugin-vue": "^10.2.0",
    "esno": "^4.8.0",
    "globals": "^16.2.0",
    "sass-embedded": "^1.89.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.34.0",
    "vite": "^6.3.5",
    "vue-tsc": "^2.2.8"
  }
}
